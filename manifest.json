{
    "manifest_version": 3,
    "version": "0.0.1",
    "name": "Bye Statistics Info",
    "description": "Removes the statistics info from Twitter Timeline",
    "author": "Gustavo Henrique",
    "action": {
        "default_title": "Bye Statistics Info",
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "permissions": [
        "activeTab",
        "storage",
        "scripting"
    ]
}